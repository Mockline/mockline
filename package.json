{
  "name": "mockline",
  "version": "0.0.8",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "postinstall": "bun run dev:prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/lucide": "^1.1.185",
    "@nuxt/fonts": "^0.6.1",
    "@nuxt/kit": "^3.11.2",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/tailwindcss": "latest",
    "@radix-ui/colors": "^3.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "defu": "^6.1.4",
    "radix-vue": "^1.7.4",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.3",
    "vue-sonner": "^1.1.2"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^1.0.0",
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/module-builder": "^0.6.0",
    "@nuxt/schema": "^3.11.2",
    "@nuxt/test-utils": "^3.12.1",
    "@types/node": "^20.12.7",
    "@vue/test-utils": "^2.4.5",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "happy-dom": "^14.7.1",
    "nuxt": "^3.11.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.2",
    "zod": "^3.23.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://mockline.dev",
  "description": "Mockline is a premium UI Kit for Nuxt and VueJS based on TailwindCSS. It provides a set of components and utilities to help you build your next project.",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "mockline",
    "vue",
    "vue3",
    "tailwindcss",
    "tailwind"
  ]
}
