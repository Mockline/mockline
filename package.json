{
  "name": "mockline",
  "version": "0.0.1",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    },
    "./nuxt": {
      "require": {
        "default": "./dist/nuxt/index.cjs",
        "types": "./dist/nuxt/index.d.cts"
      },
      "import": {
        "default": "./dist/nuxt/index.mjs",
        "types": "./dist/nuxt/index.d.mts"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "automd": "automd",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build": "automd && vite build && unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "bun run build && bun run test && bun run typecheck && bun run lint",
    "release": "bun run check && changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "radix-vue": "^1.7.0",
    "tailwind-merge": "^2.2.2"
  },
  "devDependencies": {
    "@hrcd/eslint-config": "^0.0.3",
    "@types/bun": "^1.0.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "automd": "^0.3.7",
    "autoprefixer": "^10.4.19",
    "changelogen": "^0.5.5",
    "eslint": "^9.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.11",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "vue": ">= 3.2.0"
  },
  "packageManager": "bun@1.0.30",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  }
}
