{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "stream",
  "globalDependencies": [
    "**/.env"
  ],
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ]
    },
    "dev:prepare": {
      "dependsOn": ["^dev:prepare"],
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ]
    },
    "release": {
      "dependsOn": ["^release"]
    },
    "typecheck": {},
    "test": {},
    "lint": {},
    "lint:fix": {}
  }
}
