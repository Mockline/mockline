{
  "name": "@mockline/ui",
  "version": "0.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "automd": "automd",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build": "automd && vite build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check": "bun run build && bun run test && bun run typecheck && bun run lint",
    "release": "bun run check && changelogen --release && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@types/bun": "^1.0.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "vue": ">= 3.2.0"
  },
  "packageManager": "bun@1.0.30",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  }
}
