{
  "name": "mockline",
  "version": "0.15.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "imports": {
    "#build/mockline/*": "./.nuxt/mockline/*.ts"
  },
  "style": "./dist/runtime/index.css",
  "main": "./dist/module.cjs",
  "files": [
    ".nuxt/mockline",
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "DEV=true nuxi dev playground",
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bun run lint && bun run prepack && npm publish",
    "release:edge": "bun run lint && bun run prepack && npm publish --tag edge",
    "test": "bun run typecheck && bun run lint && bun run prepack",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.26",
    "@mockline/themes": "*",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@tailwindcss/postcss": "4.0.6",
    "@tailwindcss/vite": "4.0.6",
    "defu": "^6.1.4",
    "pathe": "^2.0.3",
    "reka-ui": "^1.0.0-alpha.9",
    "scule": "^1.3.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss": "4.0.6",
    "vaul-vue": "^0.2.1",
    "vue-sonner": "^1.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "shiki-transformer-color-highlight": "^0.2.0",
    "@nuxt/content": "3.1.1",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "^22.13.1",
    "changelogen": "^0.5.7",
    "nuxt": "^3.15.4",
    "typescript": "5.7.3",
    "vue-tsc": "^2.2.0",
    "zod": "^3.24.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git",
    "directory": "packages/nuxt"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://mockline.dev",
  "description": "Mockline is a premium UI Kit for Nuxt and VueJS based on TailwindCSS. It provides a set of components and utilities to help you build your next project.",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "mockline",
    "vue",
    "vue3",
    "tailwindcss",
    "tailwind"
  ]
}
