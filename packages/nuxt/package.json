{
  "name": "mockline",
  "version": "0.16.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./unplugin": {
      "types": "./dist/unplugin.d.ts",
      "import": "./dist/unplugin.mjs",
      "require": "./dist/unplugin.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./vue-plugin": {
      "types": "./vue-plugin.d.ts"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "imports": {
    "#build/mockline/*": "./.nuxt/mockline/*.ts",
    "#build/app.config": "./.nuxt/app.config.mjs",
    "#build/mockline-image-component": "./.nuxt/mockline-image-component.ts"
  },
  "style": "./dist/runtime/index.css",
  "main": "./dist/module.cjs",
  "files": [
    ".nuxt/mockline",
    "vue-plugin.d.ts",
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "DEV=true nuxi dev playground",
    "dev:vue": "DEV=true vite playground-vue",
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && vite build playground-vue",
    "release": "bun run lint && bun run prepack && npm publish",
    "release:edge": "bun run lint && bun run prepack && npm publish --tag edge",
    "test": "bun run typecheck && bun run lint && bun run prepack",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.35",
    "@mockline/themes": "*",
    "@nuxt/fonts": "^0.11.1",
    "@nuxt/icon": "^1.12.0",
    "@nuxt/kit": "^3.16.2",
    "@nuxt/schema": "^3.16.2",
    "@nuxtjs/color-mode": "^3.5.2",
    "@tailwindcss/postcss": "4.1.3",
    "@tailwindcss/vite": "4.1.3",
    "@unhead/vue": "1.11.20",
    "defu": "^6.1.4",
    "knitwork": "^1.2.0",
    "magic-string": "^0.30.17",
    "mlly": "^1.7.4",
    "ohash": "^1.1.6",
    "pathe": "^2.0.3",
    "reka-ui": "^2.2.0",
    "scule": "^1.3.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "4.1.3",
    "vaul-vue": "^0.4.1",
    "tinyglobby": "^0.2.12",
    "ufo": "^1.6.1",
    "unplugin": "^2.3.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vue-sonner": "^1.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "shiki-transformer-color-highlight": "^0.2.0",
    "@nuxt/content": "3.4.0",
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "^22.14.0",
    "changelogen": "^0.6.1",
    "nuxt": "^3.16.2",
    "typescript": "5.8.3",
    "vue-tsc": "^2.2.8",
    "zod": "^3.24.2",
    "@vue/test-utils": "^2.4.6",
    "vitest": "^3.1.1",
    "happy-dom": "^17.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git",
    "directory": "packages/nuxt"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://mockline.dev",
  "description": "Mockline is a premium UI Kit for Nuxt and VueJS based on TailwindCSS. It provides a set of components and utilities to help you build your next project.",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "mockline",
    "vue",
    "vue3",
    "tailwindcss",
    "tailwind"
  ]
}
