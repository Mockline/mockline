{
  "name": "mockline",
  "version": "0.4.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "style": "./dist/runtime/index.css"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "style": "./dist/runtime/index.css",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "postinstall": "bun run dev:prepare",
    "dev": "DEV=true nuxi dev playground",
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bun run tailwind && bun run lint && bun run prepack && release-it",
    "release:edge": "release-it --preRelease=edge --npm.tag=edge",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@iconify-json/lucide": "^1.2.12",
    "@nuxt/content": "^2.13.4",
    "@nuxt/fonts": "^0.10.2",
    "@nuxt/icon": "^1.6.1",
    "@nuxt/kit": "^3.14.159",
    "@nuxtjs/color-mode": "^3.5.2",
    "@radix-ui/colors": "^3.0.0",
    "@tailwindcss/postcss": "4.0.0-alpha.31",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "4.0.0-alpha.31",
    "@nuxt/schema": "^3.14.159",
    "defu": "^6.1.4",
    "pathe": "^1.1.2",
    "radix-vue": "^1.9.9",
    "scule": "^1.3.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "4.0.0-alpha.31",
    "vaul-vue": "^0.2.0",
    "vue-sonner": "^1.2.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/test-utils": "^3.14.4",
    "@types/node": "^20.17.6",
    "changelogen": "^0.5.7",
    "nuxt": "^3.14.159",
    "release-it": "^17.10.0",
    "typescript": "5.6.3",
    "vue-tsc": "^2.1.10",
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mockline/mockline.git",
    "directory": "packages/mockline"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://mockline.dev",
  "description": "Mockline is a premium UI Kit for Nuxt and VueJS based on TailwindCSS. It provides a set of components and utilities to help you build your next project.",
  "keywords": [
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "mockline",
    "vue",
    "vue3",
    "tailwindcss",
    "tailwind"
  ]
}
