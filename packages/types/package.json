{
  "name": "@mockline/types",
  "version": "0.2.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "bun run build",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "release": "bun run build && npm publish"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "latest"
  }
}
