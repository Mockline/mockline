{
  "name": "@mockline/themes",
  "version": "0.15.0",
  "description": "Mockline UI component themes and variants",
  "type": "module",
  "private": false,
  "license": "MIT",
  "author": "mockline",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockline/mockline.git",
    "directory": "packages/themes"
  },
  "bugs": {
    "url": "https://github.com/mockline/mockline/issues"
  },
  "keywords": [
    "ui",
    "components",
    "themes",
    "tailwind",
    "design-system"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "style": "./dist/index.css",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "dev:prepare": "bun run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "bun run typecheck && bun run lint && bun run build",
    "prepublishOnly": "bun run clean && bun run build",
    "release": "bun run test && npm publish"
  },
  "dependencies": {
    "tailwind-variants": "^0.3.1"
  },
  "devDependencies": {
    "tsup": "8.4.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
