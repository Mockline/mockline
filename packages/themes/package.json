{
  "name": "@mockline/themes",
  "version": "0.1.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "bun run build",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "bun run build && npm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "latest",
    "@mockline/types": "workspace:*"
  }
}
